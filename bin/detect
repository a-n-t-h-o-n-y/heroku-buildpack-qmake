#!/bin/sh
# bin/detect BUILD_DIR

# Depends on heroku-buildpack-apt install of qmake
echo "BUILD_DIR ${1}";

QMAKE_EXE=/app/.apt/usr/lib/qt5/bin/qmake
echo "pwd:"
pwd;

echo "ls:";
ls;

echo "ls -a ~"
ls -a ~

echo "QMake Version:"
# ${QMAKE_EXE} --version
# exit 1;

# which ${QMAKE_EXE} >/dev/null && echo "QMake";
